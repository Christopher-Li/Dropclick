var bargraphs = document.getElementsByClassName("bargraph");
var answers = $('.temp_information').data('answers');
var bargraphvalues = []

// alert(answers.length);

for (i=0; i < bargraphs.length; i++) {
	bargraphvalues.push(answers[i].votes)
}

var maximumvalue = Math.max.apply(null, bargraphvalues);
i = 0

for (i=0; i < bargraphs.length; i++) {
	$("#graph" + i).css("width", answers[i].votes/maximumvalue*400 + "px")
	// bargraphs[i].css("width",maximumvalue/answers[i].votes*400 + "px")
}
